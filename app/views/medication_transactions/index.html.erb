<%= stylesheet_link_tag 'indexes', media: 'all', 'data-turbo-track': 'reload' %>

<h1 class="title shadow-lg">Medication Transactions</h1>

<%= link_to "Back to Medications", medications_path %>

<div id="medication_transactions">
  <% @medication_transactions.each do |medication_transaction| %>
    <%= render medication_transaction %>
    <p>
      <%= link_to "Show this medication transaction", medication_transaction %>
    </p>
  <% end %>
</div>

<%= link_to "New medication transaction", new_medication_transaction_path %>
